<UserControl x:Class="Student_Assistant.View.RoomPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Student_Assistant.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="5*"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Image Margin="20" Source="/Resources/Map.jpg"/>
        <Button Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center" Width="200" Command="{Binding BackCommand}" Content="Back" Height="60"/>
    </Grid>
</UserControl>
